<div class="project-new">
  <h1 class="p_title" style="text-align: center;" >ADD A PROJECT</h1>

  <div class="project-new-body">
    <%= simple_form_for (@project) do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
      <div class="button-new">
        <button type="button" class="btn btn-link btn_create_p"><%= f.button :submit, value: "Create" %></button>
      </div>
    <% end %>
  </div>

</div>
